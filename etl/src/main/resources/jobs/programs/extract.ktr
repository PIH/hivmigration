    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#x2f;&#x2f; Set outcome date to treatment status date if there is an outcome&#xa;&#x2f;&#x2f; If this date is null, use an approximation by taking 6 months from last visit&#xa;var OUTCOME_DATE &#x3d; null&#x3b;&#xa;var ENROLLMENT_DATE &#x3d; MIN_VISIT_DATE&#x3b;&#xa;var artStartDate &#x3d; ART_START_DATE&#x3b;&#xa;&#xa;if &#x28;OUTCOME &#x21;&#x3d; null&#x29; &#x7b;&#xa;	OUTCOME_DATE &#x3d; TREATMENT_STATUS_DATE&#x3b;&#xa;	if &#x28;OUTCOME_DATE &#x3d;&#x3d; null&#x29; &#x7b;&#xa;		if &#x28;REGIMEN_OUTCOME_DATE &#x21;&#x3d; null&#x29; &#x7b;&#xa;			OUTCOME_DATE &#x3d; REGIMEN_OUTCOME_DATE&#x3b;&#xa;		&#x7d;&#xa;	&#x7d;&#xa;	else &#x7b;&#xa;		if &#x28;REGIMEN_OUTCOME_DATE &#x21;&#x3d; null &#x26;&#x26;&#xa;			REGIMEN_OUTCOME_DATE.getTime&#x28;&#x29; &#x3e; OUTCOME_DATE.getTime&#x28;&#x29;&#x29; &#x7b;&#xa;			OUTCOME_DATE &#x3d; REGIMEN_OUTCOME_DATE&#x3b;&#xa;		&#x7d;&#xa;	&#x7d;&#xa;	if &#x28;OUTCOME_DATE &#x3d;&#x3d; null&#x29; &#x7b;&#xa;		OUTCOME_DATE &#x3d; MAX_VISIT_DATE&#x3b;&#xa;		OUTCOME_DATE.setMonth&#x28;OUTCOME_DATE.getMonth&#x28;&#x29; &#x2b; 6&#x29;&#x3b;&#xa;	&#x7d;&#xa;&#xa;	&#x2f;&#x2f; There are a small number of cases in which ART starts after the outcome &#x28;4-5&#x29;&#xa;	&#x2f;&#x2f; These seem to be data quality issues, and only affect inactive patients&#xa;	&#x2f;&#x2f; Set ART Start Date to the Outcome Date in these cases&#xa;	if &#x28;artStartDate &#x21;&#x3d; null&#x29; &#x7b;&#xa;		if &#x28;artStartDate.getTime&#x28;&#x29; &#x3e; OUTCOME_DATE.getTime&#x28;&#x29;&#x29; &#x7b;&#xa;			artStartDate &#x3d; OUTCOME_DATE&#x3b;&#xa;		&#x7d;&#xa;	&#x7d;&#xa;&#x7d;&#xa;&#xa;&#x2f;&#x2f; Enrollment Date in program is earlier of minimum encounter and art start date&#xa;if &#x28;artStartDate &#x21;&#x3d; null &#x26;&#x26; artStartDate.getTime&#x28;&#x29; &#x3c; ENROLLMENT_DATE.getTime&#x28;&#x29;&#x29; &#x7b;&#xa;	ENROLLMENT_DATE &#x3d; artStartDate&#x3b;&#xa;&#x7d;&#xa;&#xa;&#x2f;&#x2f; If outcome date is earlier than enrollment date, set it to enrollment date&#xa;if &#x28;OUTCOME_DATE &#x21;&#x3d; null &#x26;&#x26; ENROLLMENT_DATE &#x21;&#x3d; null &#x26;&#x26;&#xa;	OUTCOME_DATE.getTime&#x28;&#x29; &#x3c; ENROLLMENT_DATE.getTime&#x28;&#x29;&#x29; &#x7b;&#xa;	OUTCOME_DATE &#x3d; ENROLLMENT_DATE&#x3b;&#xa;&#x7d;</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>OUTCOME_DATE</name>
